{
  "name": "website-next",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "bun -e \"process.env.NODE_ENV !== 'production' && require('child_process').spawnSync('lefthook', ['install'], {stdio: 'inherit', shell: true})\"",
    "dev": "next dev",
    "dev:scan": "next dev & bunx react-scan localhost:3000",
    "lint": "biome check --write",
    "prebuild": "bun run next telemetry disable",
    "build": "run-script-os",
    "build:default": "NODE_OPTIONS=\"--max-old-space-size=16384\" bun run next build",
    "build:windows": "set NODE_OPTIONS=\"--max-old-space-size=16384\" && bun run next build",
    "postbuild": "mkdir -p .next/standalone/public .next/standalone/.next/static && cp -r public/* .next/standalone/public && cp -r .next/static/* .next/standalone/.next/static",
    "start": "bun run .next/standalone/server.js",
    "services:setup": "run-script-os",
    "services:setup:default": "set -e && bun run db:stop && bun run s3:stop && bun run db:delete && bun run s3:delete && bun run db:start && bun run s3:start && sleep 10 && bun run db:push && bun run db:seed",
    "services:setup:windows": "bun run db:stop && bun run s3:stop && bun run db:delete && bun run s3:delete && bun run db:start && bun run s3:start && timeout 10 && bun run db:push && bun run db:seed",
    "services:stop": "docker compose -f compose.local.yml down",
    "db:start": "docker compose -f compose.local.yml up db -d",
    "db:stop": "docker compose -f compose.local.yml down db",
    "db:delete": "bun -e \"require('fs').rmSync('data/db', { recursive: true, force: true })\"",
    "db:logs": "docker compose -f compose.local.yml logs -f db",
    "db:generate": "bunx drizzle-kit generate",
    "db:drop": "bunx drizzle-kit drop",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:push": "bunx drizzle-kit push",
    "db:studio": "bunx drizzle-kit studio",
    "db:seed": "bun run src/server/db/seed.ts",
    "s3:squash": "bun -e \"require('fs').rmSync('src/server/db/migrations/*', { recursive: true, force: true })\" && bun run db:generate",
    "s3:start": "docker compose -f compose.local.yml up s3 -d",
    "s3:stop": "docker compose -f compose.local.yml down s3",
    "s3:delete": "bun -e \"require('fs').rmSync('data/s3', { recursive: true, force: true })\"",
    "s3:logs": "docker compose -f compose.local.yml logs -f s3",
    "s3:buckets": "bun run src/server/s3/buckets.ts",
    "email:dev": "bun run email dev --dir src/emails --port 3080"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.777.0",
    "@aws-sdk/s3-request-presigner": "^3.777.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-email/components": "0.0.32",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-form": "^0.34.4",
    "@tanstack/react-query": "^5.70.0",
    "@tanstack/react-query-next-experimental": "^5.70.0",
    "@tanstack/zod-form-adapter": "^0.34.4",
    "@trpc/client": "^11.0.1",
    "@trpc/react-query": "^11.0.1",
    "@trpc/server": "^11.0.1",
    "@tsparticles/react": "^3.0.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/postcss-load-config": "^3.0.1",
    "cmdk": "1.0.0",
    "country-flag-icons": "^1.5.18",
    "cva": "^1.0.0-beta.3",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.38.4",
    "drizzle-zod": "^0.6.1",
    "input-otp": "^1.4.2",
    "libphonenumber-js": "^1.12.6",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.485.0",
    "motion": "^11.18.2",
    "next": "^15.2.4",
    "next-intl": "^3.26.5",
    "next-themes": "1.0.0-beta.0",
    "nodemailer": "^6.10.0",
    "nuqs": "^2.4.1",
    "oslo": "^1.2.1",
    "postgres": "^3.4.5",
    "react": "^19.1.0",
    "react-day-picker": "^9.6.3",
    "react-dom": "^19.1.0",
    "react-phone-number-input": "^3.4.12",
    "reading-time": "^1.5.0",
    "run-script-os": "^1.1.6",
    "sonner": "^1.7.4",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.0.2",
    "tsparticles": "^3.8.1",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@faker-js/faker": "^9.6.0",
    "@tailwindcss/postcss": "^4.0.17",
    "@types/bun": "^1.2.8",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "client-only": "^0.0.1",
    "drizzle-kit": "^0.30.6",
    "drizzle-seed": "^0.3.1",
    "lefthook": "^1.11.5",
    "postcss": "^8.5.3",
    "react-email": "3.0.6",
    "react-scan": "^0.2.14",
    "server-only": "^0.0.1",
    "tailwindcss": "^4.0.17",
    "tw-animate-css": "^1.2.5",
    "tw-clamp-css": "^1.1.1",
    "typescript": "^5.8.2"
  },
  "packageManager": "bun@1.2.0"
}
